{
  "name": "peer-files",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "move:dir": "copyfiles semantic/dist/** dist",
    "move:file": "copyfiles -f node_modules/jquery/dist/jquery.min.js dist",
    "move": "npm run move:dir && npm run move:file",
    "scripts": "rollup --config rollup.config.js",
    "watch:js": "nodemon --watch src/js --exec npm run scripts",
    "server:dev": "browser-sync start -s dist -f dist --open local",
    "dev": "npm run move && concurrently \"npm run watch:js\" \"npm run server:dev\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-preact": "^1.1.0",
    "blob-util": "^1.3.0",
    "bluebird": "^3.5.0",
    "copyfiles": "^1.2.0",
    "gulp": "^3.9.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "pako": "^1.0.6",
    "preact": "^8.2.5",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "semantic-ui": "^2.2.13"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "concurrently": "^3.5.0",
    "nodemon": "^1.12.0"
  }
}
